REPORT zshow_fm_interface.

DATA: lt_params TYPE TABLE OF rsexfdir,
      ls_param  TYPE rsexfdir.

PARAMETERS p_func TYPE rs38l_fnam OBLIGATORY.

CALL FUNCTION 'FUNCTION_IMPORT_INTERFACE'
  EXPORTING
    funcname   = p_func
  TABLES
    parameters = lt_params.

LOOP AT lt_params INTO ls_param.
  WRITE: / ls_param-parameter, ls_param-paramtype, ls_param-paramclass, ls_param-paramtypekind.
ENDLOOP.