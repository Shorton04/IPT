REPORT ztest_cmx_calib_result.

DATA: lv_status     TYPE char20,
      lv_message    TYPE char255,
      lv_notif_no   TYPE qmnum VALUE '200000123',
      lv_orderid    TYPE aufnr VALUE '60000150',
      lv_operation  TYPE vornr VALUE '0010',
      lv_cert_no    TYPE char40 VALUE 'CALIB-20251013', "adjust this TYPE if needed
      lv_date_time  TYPE tzntstmpl VALUE '20251013153000',
      lv_userid     TYPE char20 VALUE 'TECH001',
      lv_passed     TYPE char1 VALUE 'X'.

CALL FUNCTION 'ZBAPI_CMX_CALIB_RESULT'
  EXPORTING
    iv_notif_no  = lv_notif_no
    iv_orderid   = lv_orderid
    iv_operation = lv_operation
    iv_cert_no   = lv_cert_no
    iv_date_time = lv_date_time
    iv_userid    = lv_userid
    iv_passed    = lv_passed
  IMPORTING
    ev_status    = lv_status
    ev_message   = lv_message.

WRITE: / 'BAPI Result Status:', lv_status,
       / 'Message:', lv_message.