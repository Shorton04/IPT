REPORT ztest_cmx_calib_result.

DATA: lv_status  TYPE char20,
      lv_message TYPE char255.

CALL FUNCTION 'ZBAPI_CMX_CALIB_RESULT'
  EXPORTING
    iv_notif_no  = '200000123'
    iv_orderid   = '60000150'
    iv_operation = '0010'
    iv_cert_no   = 'CALIB-20251013'
    iv_date_time = '20251013153000'
    iv_userid    = sy-uname
    iv_passed    = 'X'
  IMPORTING
    ev_status    = lv_status
    ev_message   = lv_message.

WRITE: / 'Status:', lv_status,
       / 'Message:', lv_message.